  "scripts": {
    "start": "serve -l 3000 src",
    "test": "npm run eslint",
    "eslint": "eslint 'src/**/*.js' 'test/**/*.js' 'test-sample-solution/**/*.js' 'tutorial-snippets/**/*.js'",
    "test:sample-solution": "npm run eslint && start-test 3000 cypress:run:sample-solution",
    "cypress:run:sample-solution": "cypress run --config integrationFolder=test-sample-solution --browser chrome",
    "cypress:open:sample-solution": "cypress open --config integrationFolder=test-sample-solution",
    "mocha:sample-solution": " mocha -r esm 'test-sample-solution/it/*.js' 'test-sample-solution/unit/*.js'"
  },
